<?xml version="1.0" encoding="UTF-8"?>
<ejemploDC4:DiagramaClases
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ejemploDC4="http://www.example.org/ejemploDC4"
    xsi:schemaLocation="http://www.example.org/ejemploDC4 ejemploDC4.ecore">
  <paquetes_contenidos
      nombre="entity">
    <clasificadores_contenidos
        xsi:type="ejemploDC4:Clase"
        nombre="Categoria">
      <atributos_contenidos
          nombre="id"
          tipo="Long"/>
      <atributos_contenidos
          nombre="denominacion"/>
      <operaciones_contenidas
          nombre="implementarCategoria"
          visibilidad="public"/>
      <operaciones_contenidas
          nombre="prueba"
          visibilidad="public"/>
      <implementaciones_contenidas
          origen="//@paquetes_contenidos.0/@clasificadores_contenidos.0"
          destino="//@paquetes_contenidos.0/@clasificadores_contenidos.6"/>
    </clasificadores_contenidos>
    <clasificadores_contenidos
        xsi:type="ejemploDC4:Clase"
        nombre="Articulo">
      <atributos_contenidos
          nombre="id"
          tipo="Long"/>
      <atributos_contenidos
          nombre="denominacion"/>
      <atributos_contenidos
          nombre="precio"
          tipo="int"/>
      <atributos_contenidos
          nombre="cantidad"
          tipo="int"/>
      <implementaciones_contenidas
          origen="//@paquetes_contenidos.0/@clasificadores_contenidos.1"
          destino="//@paquetes_contenidos.0/@clasificadores_contenidos.6"/>
      <relaciones_contenidas
          rol="cat"
          origen="//@paquetes_contenidos.0/@clasificadores_contenidos.1"
          destino="//@paquetes_contenidos.0/@clasificadores_contenidos.0"/>
    </clasificadores_contenidos>
    <clasificadores_contenidos
        xsi:type="ejemploDC4:Clase"
        nombre="Factura">
      <atributos_contenidos
          nombre="id"
          tipo="Long"/>
      <atributos_contenidos
          nombre="fecha"/>
      <atributos_contenidos
          nombre="numero"
          tipo="int"/>
      <atributos_contenidos
          nombre="total"
          tipo="int"/>
      <implementaciones_contenidas
          origen="//@paquetes_contenidos.0/@clasificadores_contenidos.2"
          destino="//@paquetes_contenidos.0/@clasificadores_contenidos.6"/>
      <relaciones_contenidas
          rol="df"
          origen="//@paquetes_contenidos.0/@clasificadores_contenidos.2"
          destino="//@paquetes_contenidos.0/@clasificadores_contenidos.4"
          tipo="composicion"/>
      <relaciones_contenidas
          origen="//@paquetes_contenidos.0/@clasificadores_contenidos.2"
          destino="//@paquetes_contenidos.0/@clasificadores_contenidos.5"/>
    </clasificadores_contenidos>
    <clasificadores_contenidos
        xsi:type="ejemploDC4:Clase"
        nombre="Domicilio">
      <atributos_contenidos
          nombre="id"
          tipo="Long"/>
      <atributos_contenidos
          nombre="nombreCalle"/>
      <atributos_contenidos
          nombre="numero"
          tipo="int"/>
      <implementaciones_contenidas
          origen="//@paquetes_contenidos.0/@clasificadores_contenidos.3"
          destino="//@paquetes_contenidos.0/@clasificadores_contenidos.6"/>
    </clasificadores_contenidos>
    <clasificadores_contenidos
        xsi:type="ejemploDC4:Clase"
        nombre="DetalleFactura">
      <atributos_contenidos
          nombre="id"
          tipo="Long"/>
      <atributos_contenidos
          nombre="cantidad"
          tipo="int"/>
      <atributos_contenidos
          nombre="subtotal"
          tipo="int"/>
      <implementaciones_contenidas
          origen="//@paquetes_contenidos.0/@clasificadores_contenidos.4"
          destino="//@paquetes_contenidos.0/@clasificadores_contenidos.6"/>
      <relaciones_contenidas
          rol="art"
          origen="//@paquetes_contenidos.0/@clasificadores_contenidos.4"
          destino="//@paquetes_contenidos.0/@clasificadores_contenidos.1"
          multiplicidad="OneToMany"/>
    </clasificadores_contenidos>
    <clasificadores_contenidos
        xsi:type="ejemploDC4:Clase"
        nombre="Cliente">
      <herencias_contenidas
          origen="//@paquetes_contenidos.0/@clasificadores_contenidos.5"
          destino="//@paquetes_contenidos.0/@clasificadores_contenidos.2"/>
      <atributos_contenidos
          nombre="id"
          tipo="Long"/>
      <atributos_contenidos
          nombre="nombre"/>
      <atributos_contenidos
          nombre="apellido"/>
      <atributos_contenidos
          nombre="dni"
          tipo="int"/>
      <operaciones_contenidas
          nombre="comprar"
          visibilidad="public"/>
      <operaciones_contenidas
          nombre="esCasado"
          visibilidad="public"
          retorno="boolean"/>
      <implementaciones_contenidas
          origen="//@paquetes_contenidos.0/@clasificadores_contenidos.5"
          destino="//@paquetes_contenidos.0/@clasificadores_contenidos.6"/>
      <relaciones_contenidas
          origen="//@paquetes_contenidos.0/@clasificadores_contenidos.5"
          destino="//@paquetes_contenidos.0/@clasificadores_contenidos.3"/>
    </clasificadores_contenidos>
    <clasificadores_contenidos
        xsi:type="ejemploDC4:Interface"
        nombre="Serializable"/>
  </paquetes_contenidos>
</ejemploDC4:DiagramaClases>
